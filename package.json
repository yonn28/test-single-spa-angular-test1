    "@angular/platform-browser-dynamic": "~12.1.2",
    "@angular/router": "~12.1.2",
    "@auth0/angular-jwt": "^5.0.2",
    "@ngrx/effects": "^12.3.0",
    "@ngrx/entity": "^12.3.0",
    "md5-typescript": "^1.0.5",
    "ngx-webstorage": "^5.0.0",
    "node-forge": "^0.10.0",
