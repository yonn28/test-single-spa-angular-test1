{
  "name": "org",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:local": "ng build bbog-ae-bnpl-proposal-web-ui --configuration development --deploy-url http://localhost:4200/",
    "build:enviroment": "ng build org --configuration qa --deploy-url http://",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --no-watch --code-coverage",
    "test:debug": "ng test --browsers ChromeDebug ",
    "build:single-spa:org": "ng build org --prod",
    "serve:single-spa:org": "ng s --project org --disable-host-check --port 4200 --live-reload false"
  },
  "private": true,
